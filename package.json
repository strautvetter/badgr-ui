        "fontfaceobserver": "^2.0.9",
        "handlebars-helpers": "^0.10.0",
        "html2canvas": "^1.4.1",
        "inert-polyfill": "^0.2.5",
        "intl": "1.2.5",
        "jspdf": "^2.5.1",
        "maplibre-gl": "3.6.2",
        "marked": "^11.0.0",
        "popper.js": "^1.15.0",
        "postcss-cssnext": "3.1.1",
